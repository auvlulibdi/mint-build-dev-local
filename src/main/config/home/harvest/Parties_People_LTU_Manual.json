{
    "harvester": {
        "type": "csv",
        "csv": {
            "fileLocation": "${fascinator.home}/data/Parties_People_LTU_Manual.csv",
            "idColumn": "ID",
            "recordIDPrefix": "",
            "headerRow": true,
            "delimiter": ",",
            "ignoredFields": ["Description","sidnum","sStudid"],
            "includedFields" : ["ID", "Given_Name", "Other_Names", "Family_Name", "Pref_Name", "Honorific", "Email", "Job_Title", "GroupID_1", "GroupID_2", "GroupID_3", "ANZSRC_FOR_1", "ANZSRC_FOR_2", "ANZSRC_FOR_3", "URI", "NLA_Party_Identifier", "ResearcherID", "openID", "Personal_URI", "Personal_Homepage", "Staff_Profile_Homepage", "ctype", "lcurrent", "sGender", "ANZSRC_FOR_1_PCT", "ANZSRC_FOR_2_PCT", "ANZSRC_FOR_3_PCT"]
        }
    },

    "transformer": {
        "curation": ["local"],
        "metadata": ["ingest-relations", "jsonVelocity"]
    },
    "curation": {
        "neverPublish": false,
        "alreadyCurated": false
    },

    "transformerOverrides": {
        "ingest-relations": {
            "sourcePid": "metadata.json",
            "sourcePath": ["data"],
            "relations": {
                "GroupID_1": {
                    "prefix": "",
                    "relation": "isMemberOf",
                    "reverseRelation": "hasMember"
                },
                "GroupID_2": {
                    "prefix": "",
                    "relation": "isMemberOf",
                    "reverseRelation": "hasMember"
                },
                "GroupID_3": {
                    "prefix": "",
                    "relation": "isMemberOf",
                    "reverseRelation": "hasMember"
                }
            }
        },
        "local": {
            "template": "${server.url.base}published/detail/[[OID]]"
        },
        "jsonVelocity": {
            "templatesPath" : "${fascinator.home}/templates/people",
            "portalId": "Parties_People"
        }
    },

    "indexer": {
        "script": {
            "type": "python",
            "rules": "Parties_People_LTU.py"
        },
        "params": {
            "repository.name": "People",
            "repository.type": "Parties"
        }
    }
}
